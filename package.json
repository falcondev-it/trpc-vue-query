{
  "name": "@falcondev-oss/trpc-vue-query",
  "type": "module",
  "version": "0.5.0-rc.17",
  "description": "A tRPC wrapper around '@tanstack/vue-query'",
  "license": "MIT",
  "repository": "github:falcondev-oss/trpc-vue-query",
  "bugs": {
    "url": "https://github.com/falcondev-oss/trpc-vue-query/issues"
  },
  "keywords": [
    "trpc",
    "vue-query",
    "trpc-client",
    "tanstack-query",
    "typescript"
  ],
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "22",
    "pnpm": "9"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint --cache . && prettier --check --cache .",
    "lint:ci": "eslint --cache --cache-strategy content . && prettier --check --cache --cache-strategy content .",
    "lint:fix": "eslint --fix --cache . && prettier --write --cache .",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "prepare": "husky",
    "test": "start-server-and-test test:server http-get://localhost:3000/ping vitest",
    "test:server": "tsx ./test/trpc/index.ts"
  },
  "peerDependencies": {
    "@tanstack/vue-query": "^5.22.2",
    "@trpc/client": "next",
    "@trpc/server": "next",
    "vue": "^3.4.19"
  },
  "dependencies": {
    "@vueuse/core": "^11.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@eslint/eslintrc": "^3.2.0",
    "@louishaftmann/commitlint-config": "^4.0.3",
    "@louishaftmann/eslint-config": "^4.0.3",
    "@louishaftmann/lintstaged-config": "^4.0.3",
    "@louishaftmann/prettier-config": "^4.0.3",
    "@tanstack/vue-query": "^5.62.3",
    "@trpc/client": "11.0.0-rc.660",
    "@trpc/server": "11.0.0-rc.660",
    "@types/eslint": "^9.6.1",
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/ws": "^8.5.13",
    "@vitest/ui": "^2.1.8",
    "eslint": "~9.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "start-server-and-test": "^2.0.8",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "type-fest": "^4.30.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "vue": "^3.5.13",
    "ws": "^8.18.0",
    "zod": "^3.24.0"
  },
  "changelogithub": {
    "extends": "gh:falcondev-it/configs/changelogithub"
  },
  "pnpm": {
    "overrides": {
      "eslint": "~9.14.0"
    }
  }
}
